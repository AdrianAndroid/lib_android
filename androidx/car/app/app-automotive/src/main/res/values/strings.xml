<resources>
    <!-- Action titles -->
    <!-- Title of a button that allows the user to exit the application [CHAR_LIMIT=50] -->
    <string name="error_action_finish">Close application</string>
    <!-- Title of a button that allows the user to check for updates [CHAR_LIMIT=50] -->
    <string name="error_action_update_host">Check for updates</string>
    <!-- Title of a button that allows the user to retry connecting to the system [CHAR_LIMIT=50] -->
    <string name="error_action_retry">Retry</string>

    <!-- Error messages -->
    <!-- Message displayed when there is an error on the client side [CHAR_LIMIT=200] -->
    <string name="error_message_client_side_error">Application error. Please report this error to
        the app developer</string>
    <!-- Message displayed when there is an error on the host side [CHAR_LIMIT=200] -->
    <string name="error_message_host_error">System error</string>
    <!-- Message displayed when the connection to the host is interrupted [CHAR_LIMIT=200] -->
    <string name="error_message_host_connection_lost">System temporarily unavailable</string>
    <!-- Message displayed when no host is found in the system [CHAR_LIMIT=200] -->
    <string name="error_message_host_not_found">System requires update</string>
    <!-- Message displayed when the host is not compatible with this client [CHAR_LIMIT=200] -->
    <string name="error_message_host_incompatible">System requires update</string>
    <!-- Message displayed when there are multiple hosts in the system [CHAR_LIMIT=200] -->
    <string name="error_message_multiple_hosts">Incompatible system</string>
    <!-- Message displayed when an unknown error is detected [CHAR_LIMIT=200] -->
    <string name="error_message_unknown_error">Unknown error</string>
</resources>