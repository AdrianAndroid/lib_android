<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">
    <!-- String describing a complication that has not been given permission yet, which is read aloud when focused using accessibility gestures [CHAR LIMIT=35] -->
    <string name="a11y_no_permission">No permission to access data</string>
    <!-- String describing a complication that does not display any data or has not yet loaded, which is read aloud when focused using accessibility gestures [CHAR LIMIT=35] -->
    <string name="a11y_no_data">No data</string>

    <!-- Template to render Accessibility descriptions for the "Range" Complication type (e.g. a step count goal: "654 of 10000 steps")  [CHAR LIMIT=35] -->
    <string name="a11y_template_range"><xliff:g id="value" example="654">%1$f</xliff:g> of <xliff:g id="max" example="10000">%2$f</xliff:g></string>
</resources>
