# A box which contains multiple children should just layer them on top of each
# other.
box {
  contents {
    image {
      resource_id {
        value: "android"
      }
      width {
        linear_dimension {
          value: 18
        }
      }
      height {
        linear_dimension {
          value: 18
        }
      }
    }
  }

  contents {
    text {
      text {
        value: "Hello World"
      }
    }
  }
}
